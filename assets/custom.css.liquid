body {
  /*   background-color: #1B1B1B; */
  background-color: #000000;
  color: #fff;
  line-height: 1.2;
  font-family: "Palatino", Helvetica, Arial, sans-serif;
  font-weight: normal;
  font-style: normal;
  font-size: 14px;
}
body.overflow-hidden {
  overflow: hidden;
}

a {
  color: #fff;
  text-decoration: none;
}
a:hover,
a:focus {
  color: #fff;
  text-decoration: underline;
}

.container-fluid {
  width: 91%;
  /* width:980px before */
  margin: 0 auto;
}

.link {
  font-size: 10px;
  font-family: Termina, Helvetica, Arial, sans-serif;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 2px;
}

.link-underlined {
  text-decoration: underline;
}

.link-underlined:hover {
  text-decoration: none;
}

.with-back-arrow svg {
  margin-right: 5px;
}

#PageContainer {
  overflow: hidden;
}

header {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 104;
}
header.sticky {
  background-color: #000;
}
header .logo {
  width: 160px;
  margin-top: 5px;
  margin-bottom: 15px;
  margin-left: 50px;
}
header.small {
  position: fixed;
  z-index: 104;
  background-color: #000;
}
header .logo img {
  width: 100%;
  height: auto;
}
header.small .logo {
  width: 105px;
  margin: 15px 30px;
  transition: all 250ms linear;
  position: relative;
  top: auto;
  left: auto;
  transform: none;
}
header.small.sticky .logo {
  width: 65px;
  margin: 10px 30px;
}
header .cart {
  position: absolute;
  top: 30px;
  right: 130px;
  z-index: 103;
}
header .cart span {
  position: absolute;
  top: -6px;
  right: -10px;
  background: #ff0000;
  border-radius: 50%;
  font-size: 12px;
  line-height: 20px;
  min-width: 18px;
  height: 18px;
  text-align: center;
  padding: 0 4px;
}
header.small .cart {
  top: 32px;
  right: 80px;
  transition: all 250ms linear;
}
header.small.sticky .cart {
  top: 18px;
}
header.small .cart svg {
  width: 14px;
  height: 14px;
}
header.small .cart span {
  top: -8px;
  right: -12px;
  font-size: 10px;
  line-height: 18px;
  min-width: 16px;
  height: 16px;
}
header .nav {
  position: absolute;
  /* position:fixed  like line-245*/
  top: 30px;
  right: 60px;
  z-index: 103;
  cursor: pointer;
}
header.small .nav {
  top: 30px;
  right: 30px;
  transition: all 250ms linear;
}
header.small.sticky .nav {
  top: 16px;
}
header .nav svg {
  width: 36px;
  height: 36px;
  position: relative;
  top: -4px;
}
header.small .nav svg {
  width: 28px;
  height: 28px;
}
header .nav .close {
  display: none;
}
.navigation {
  position: fixed;
  top: 0;
  right: -300px;
  width: 300px;
  height: 100vh;
  padding-top: 120px;
  background-color: #000;
  z-index: 103;
  padding-left: 0;
  list-style: none;
}
.navigation a:not(.become-a-member) {
  display: block;
  padding: 15px 20px;
  border-bottom: 1px solid rgba(255, 255, 255, 0.2);
  text-transform: uppercase;
}
.navigation a:not(.become-a-member):hover,
.navigation a:not(.become-a-member):focus {
  text-decoration: none;
  background-color: #fff;
  color: #000;
}
.navigation li:last-child a:not(.become-a-member) {
  border-bottom: 0;
}
.navigation + .navigation-overlay {
  content: "";
  display: block;
  position: fixed;
  top: 0;
  left: 0;
  width: 0;
  height: 0;
  background-color: #000;
  opacity: 0;
  z-index: 102;
  transition: opacity 0.2s;
}
.navigation.active + .navigation-overlay {
  opacity: 0.75;
  width: 100%;
  height: 100%;
  /* position: fixed */
}

.home-splash {
  width: 100%;
  background-image: url({{"Cierto-Agave-Field-Splash-Photo-BW_Hero.jpeg"|asset_url}});
  background-size: cover;
  background-position: center center;
  background-repeat: no-repeat;
  background-color: #000;
  position: relative;
}
.home-splash::after {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.4);
}
.home-splash .content {
  padding-top: 170px;
  padding-bottom: 50px;
  width: 100%;
  position: relative;
  z-index: 102;
}
.home-splash .content .logo {
  width: 320px;
  margin: 0 auto 50px;
}
.home-splash .content .logo img {
  width: 100%;
}
.home-splash .content h1 {
  /*font-size: 35px;*/
  font-size: 25px;
  text-transform: uppercase;
  color: #fff;
  letter-spacing: 10px;
  margin-bottom: 50px;
  /*border: 3px solid;
  border-image-slice: 1;
  border-image-source: linear-gradient(135deg,#DFD3AD 40%,#B4975B 60%);*/
  border: 3px solid #bca57f;
  display: inline-block;
  padding: 10px 20px;
}
.home-splash .content .bottles img {
  width: 100%;
  max-width: 870px;
}
.home-splash .content .bottles {
  margin-bottom: 50px;
  padding: 0 40px;
}
.home-splash .bottom,
.home-splash .content .quote {
  width: 100%;
  /*font-size: 20px;*/
  font-size: 25px;
  font-style: italic;
  z-index: 99;
  padding-bottom: 30px;
}
.home-splash .bottom span,
.home-splash .content .quote span {
  font-style: normal;
  font-size: 14px;
  font-family: "Termina", Helvetica, Arial, sans-serif;
}

.home-divider {
  display: none;
}
.home-divider img {
  width: 100%;
}

.image-text {
  width: 100%;
  height: 80vh;
  background-color: #000;
  background-position: center center;
  background-size: cover;
  background-repeat: no-repeat;
  position: relative;
  overflow: hidden;
}
.image-text .desktop,
.image-text .mobile {
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.image-text .mobile {
  height: auto;
  display: none;
}
.image-text .row {
  margin: 0;
}
.image-text .content {
  position: absolute;
  top: 50%;
  left: calc(50% - 320px);
  transform: translate(-50%, -50%);
  height: 80%;
  display: flex;
  justify-content: center;
  align-items: center;
  max-width: 550px;
  max-height: 500px;
  padding: 10px;
  background-color: #000;
  z-index: 1;
}
.image-text .content-border {
  border: 1px solid #bca57f;
  height: 100%;
  display: flex;
  align-items: center;
}
.image-text .content.right {
  left: auto;
  right: calc(50% - 320px);
  transform: translate(50%, -50%);
  background-color: #1f1f1f;
}
.image-text .content .content-info {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
}
.image-text .content .content-info div {
  width: 100%;
}
.become-a-member {
  position: absolute;
  bottom: 40px;
  left: 10px;
}
.image-text .content .content-info .button,
.become-a-member.button,
.home-splash .content .button,
.featured-product .details .product-details-adc .button,
.product-template .details .product-details-adc .button,
.awards .button,
.image-text .content .content-info .button:focus,
.become-a-member.button:focus,
.home-splash .content .button:focus,
.featured-product .details .product-details-adc .button:focus,
.product-template .details .product-details-adc .button:focus,
.awards .button:focus,
.template-collection .more-products .products .product form .button,
.template-collection .more-products .products .product form .button:hover {
  border-color: #bca57f;
  padding: 3px;
}
.template-collection .more-products .products .product form .button {
  display: block;
  width: 100%;
  max-width: 159px;
  margin: 0 auto;
  /* opacity: 100%; */
}
.image-text .content .content-info .button span,
.become-a-member.button span,
.home-splash .content .button span,
.featured-product .details .product-details-adc .button span,
.product-template .details .product-details-adc .button span,
.awards .button span,
.template-collection .more-products .products .product form .button span {
  border: 1px solid #fff;
  padding: 15px 20px;
  display: block;
  transition: all 250ms linear;
  width: 100%;
}
.image-text .content .content-info .button:hover,
.become-a-member.button:hover,
.home-splash .content .button:hover,
.featured-product .details .product-details-adc .button:hover,
.product-template .details .product-details-adc .button:hover,
.awards .button:hover,
.template-collection .more-products .products .product form .button:hover {
  background: transparent;
  color: #fff;
  border-color: #bca57f;
}
.image-text .content .content-info .button:hover span,
.become-a-member.button:hover span,
.home-splash .content .button:hover span,
.featured-product .details .product-details-adc .button:hover span,
.product-template .details .product-details-adc .button:hover span,
.awards .button:hover span,
.template-collection .more-products .products .product form .button:hover span {
  background-color: #bcb4af;
  color: #000;
}
.image-text .content img {
  max-width: 325px;
}
.image-text .content img,
.image-text .content h4 {
  margin-bottom: 30px;
}
.image-text .content h4 {
  font-size: 14px;
  text-transform: uppercase;
  color: #fff;
  letter-spacing: 5px;
}
.image-text .cover {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transform: scale(1.5);
  transition: all 1000ms linear;
}
.image-text .cover.visible {
  transform: scale(1);
}

